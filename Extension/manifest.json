{
    "name": "StatBinge",
    "version": "1.0.0",
    "description": "Chrome Extension for StatBinge",
    "manifest_version": 3,
    "author": "Jacob Parvin",
    "action":{
        "default_popup": "popup.html",
        "default_icon" :{
            "16": "/images/logo_16.png",
            "32": "/images/logo_32.png",
            "48": "/images/logo_48.png",
            "128": "/images/logo_128.png"
        },
        "default_title": "StatBinge Data"
    },
    "background":{
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "icons": {
        "16": "/images/logo_16.png",
        "32": "/images/logo_32.png",
        "48": "/images/logo_48.png",
        "128": "/images/logo_128.png"  
    }
}